<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100"
    >

    <group>

        <clip-path
            android:pathData="M0 0H100V100H0V0Z"
            />

        <group
            android:translateX="1"
            android:translateY="0"
            android:pivotX="50"
            android:pivotY="50"
            android:scaleX="2"
            android:scaleY="2"
            android:rotation="90"
            >

            <path
                android:pathData="M0 0V100H100V0"
                >

                <aapt:attr name="android:fillColor">

                    <gradient
                        android:type="linear"
                        android:startX="25"
                        android:startY="50"
                        android:endX="75"
                        android:endY="50"
                        >

                        <item
                            android:color="#CFF0E3"
                            android:offset="0"
                            />

                        <item
                            android:color="#E6F3EE"
                            android:offset="0.297"
                            />

                    </gradient>

                </aapt:attr>
            </path>
        </group>
    </group>
</vector>